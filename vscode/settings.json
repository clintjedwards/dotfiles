{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "octref.vetur"
  },
  "cSpell.enableFiletypes": [
    "vue"
],
  "cSpell.userWords": [
    "gohcl",
    "grpc",
    "hashicorp",
    "pluggable",
    "rpcs",
    "schedulable",
    "semver"
  ],
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "editor.cursorBlinking": "smooth",
  "editor.cursorStyle": "line-thin",
  "editor.detectIndentation": false,
  "editor.fontFamily": "'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback','Font Awesome 5 Brands', 'Font Awesome 5 Free', 'Font Awesome 5 Free Solid'",
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "editor.minimap.enabled": false,
  "editor.occurrencesHighlight": false,
  "editor.renderIndentGuides": false,
  "editor.renderWhitespace": "none",
  "editor.rulers": [
    100
  ],
  "editor.snippetSuggestions": "none",
  "editor.tabSize": 4,
  "explorer.compactFolders": false,
  "explorer.openEditors.visible": 0,
  "extensions.ignoreRecommendations": true,
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.associations": {
    "*.tf": "terraform"
  },
  "go.buildOnSave": "off",
  "go.lintFlags": [
    "--enable", "golint"
  ],
  "go.lintTool": "golangci-lint",
  "go.useCodeSnippetsOnFunctionSuggest": true,
  "go.useCodeSnippetsOnFunctionSuggestWithoutType": true,
  "go.useLanguageServer": true,
  "gopls": {
    "usePlaceholders": true
  },
  "javascript.updateImportsOnFileMove.enabled": "never",
  "prettier.useTabs": false,
  "protoc": {
    "options": [
      "--proto_path=proto"
    ]
  },
  "rust-analyzer.inlayHints.enable": false,
  "rust-analyzer.lens.enable": false,
  "todohighlight.include": [
    "**/*.js",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx",
    "**/*.html",
    "**/*.php",
    "**/*.css",
    "**/*.scss",
    "**/*.go"
  ],
  "todohighlight.keywordsPattern": "TODO(.*):",
  "window.menuBarVisibility": "default",
  "window.zoomLevel": 0,
  "workbench.colorTheme": "Eastwood Dark",
  "workbench.editor.showTabs": false,
"workbench.iconTheme": "material-icon-theme",
"[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
},
}
